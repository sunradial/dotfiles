// Place your key bindings in this file to overwrite the defaults
[
	{ "key": "alt+/",  "command": "editor.action.triggerSuggest","when": "editorTextFocus" },
	{ "key": "ctrl+d", "command": "editor.action.cutLines","when": "editorTextFocus" },
	{ "key": "ctrl+d","command": "editor.action.deleteLines","when": "editorTextFocus" },
	{ "key": "f3","command": "editor.action.goToDeclaration","when": "editorHasDefinitionProvider && editorTextFocus" },
	{ "key": "alt+left", "command": "workbench.action.navigateBack" },
	{ "key": "alt+right", "command": "workbench.action.navigateForward" },
	{"key": "ctrl+k ctrl+k", "command": "editor.action.showHover"}
]